{
    "require": {
        "laravel/laravel": "^5.8",
        "ejarnutowski/laravel-api-key": "^1.0",
        "hashids/hashids": "^3.0",
        "dingo/api": "^2.4",
        "tymon/jwt-auth": "dev-develop",
        "doctrine/dbal": "^2.9",
        "yajra/laravel-datatables": "^1.5",
        "barryvdh/laravel-snappy": "^0.4.6",
        "needim/noty": "^3.1",
        "ext-json": "*",
        "ext-curl": "*",
        "maatwebsite/excel": "^3.1",
        "milon/barcode": "^5.1",
        "filp/whoops": "^2.7",
      "ext-mysqli": "*",
        "vinkla/hashids": "^9.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "ext-json": "*",
        "ext-curl": "*",
        "mpociot/laravel-apidoc-generator": "^3.17",
        "squizlabs/php_codesniffer": "^3.5",
        "mockery/mockery": "^1.0",
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-rest": "^1.2",
        "codeception/module-laravel5": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    }
}
